{
  "name": "react-sudoku",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "12.13.1",
    "npm": "6.12.1"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
